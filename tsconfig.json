{
    "compilerOptions": {
      "target": "ESNext",
      "module": "es2022",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "noImplicitAny": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./server",
      "paths": {
          "/*": ["static/*"],
      },
      "lib": [
        "es2022",
        "dom"
      ]
    }
  }